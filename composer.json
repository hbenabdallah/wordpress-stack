{
    "name": "totipot/website",
    "description": "Totipot website application based on WordPress",
    "license": "proprietary",
    "type": "project",
    "keywords": ["website", "totipot", "php", "wordpress"],
    "authors": [
        {
            "name": "Houssem eddine BEN ABDALLAH",
            "email": "hbenabdallah@smart-team.tn",
            "role": "developer"
        }
    ],
    "repositories": [],
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Units\\": "tests/units/src",
            "Tests\\Functionals\\": "tests/functionals/bootstrap"
        }
    },
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phpro/grumphp": "v0.15.2",
        "phpunit/phpunit": "^6.0",
        "sebastian/phpcpd": "3.0.1",
        "friendsofphp/php-cs-fixer": "*",
        "sensiolabs/security-checker": "^5.0",
        "squizlabs/php_codesniffer": "^2.5",
        "roave/security-advisories": "dev-master@dev"
    },
    "scripts": {},
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
